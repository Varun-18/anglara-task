{
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ]
}